[
  {
    "type": "console.flag/model",
    "properties": {
      "flag": "CAMEL_APP_FLAG",
      "model": {
        "group": "camel.apache.org",
        "version": "v1alpha1",
        "kind": "CamelApp"
      }
    }
  },
  {
    "type": "console.page/route",
    "flags": {
      "required": [
        "CAMEL_APP_FLAG"
      ]
    },
    "properties": {
      "exact": false,
      "path": "/camel/app/ns/:ns/name/:name",
      "component": {
        "$codeRef": "CamelApp"
      }
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": [
        "/camel/ns/:ns",
        "/camel/all-namespaces"
      ],
      "component": {
        "$codeRef": "CamelAppList"
      }
    }
  },
  {
    "type": "console.navigation/href",
    "flags": {
      "required": [
        "CAMEL_APP_FLAG"
      ]
    },
    "properties": {
      "id": "camel-standalone",
      "name": "%plugin__camel-dashboard-console~Camel%",
      "href": "/camel",
      "perspective": "dev",
      "section": "resources",
      "namespaced": true
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "camel-workload",
      "name": "%plugin__camel-dashboard-console~Camel%",
      "href": "/camel",
      "perspective": "admin",
      "section": "workloads",
      "namespaced": true
    }
  }
]