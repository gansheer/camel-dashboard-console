{
    "name": "camel-openshift-console-plugin",
    "version": "0.0.1",
    "private": true,
    "repository": "git@github.com:camel-tooling/camel-openshift-console-plugin.git",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "yarn clean && NODE_ENV=production yarn ts-node node_modules/.bin/webpack",
        "build-dev": "yarn clean && yarn ts-node node_modules/.bin/webpack",
        "start": "yarn ts-node node_modules/.bin/webpack serve",
        "start-console": "./start-console.sh",
        "i18n": "./i18n-scripts/build-i18n.sh && node ./i18n-scripts/set-english-defaults.js",
        "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
        "lint": "eslint ./src ./integration-tests --fix &&  stylelint \"src/**/*.css\" --allow-empty-input --fix"
    },
    "devDependencies": {
        "@babel/core": "7.18.13",
        "@babel/preset-env": "^7.22.0",
        "@openshift-console/dynamic-plugin-sdk": "1.6.0",
        "@openshift-console/dynamic-plugin-sdk-webpack": "1.2.0",
        "@patternfly/patternfly": "^5.3.1",
        "@patternfly/react-core": "^5.3.3",
        "@patternfly/react-icons": "^5.3.2",
        "@patternfly/react-styles": "^5.3.1",
        "@patternfly/react-charts": "^7.1.2",
        "@patternfly/react-log-viewer": "^5.0.0",
        "@patternfly/react-table": "^5.3.2",
        "@types/node": "^20.14.9",
        "@types/react": "^17.0.37",
        "@types/react-helmet": "^6.1.4",
        "@types/react-router-dom": "^5.3.2",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "copy-webpack-plugin": "^6.4.1",
        "css-loader": "^6.7.1",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.1",
        "i18next": "^23.11.5",
        "i18next-parser": "^8.9.0",
        "mocha-junit-reporter": "^2.2.0",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.3.0",
        "node-sass": "^9.0.0",
        "pluralize": "^8.0.0",
        "prettier": "^2.7.1",
        "prettier-stylelint": "^0.4.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.18.6",
        "react-router": "5.3.x",
        "react-router-dom": "5.3.x",
        "sass-loader": "^14.0.0",
        "style-loader": "^3.3.1",
        "stylelint": "^15.3.0",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-standard": "^31.0.0",
        "ts-loader": "^9.3.1",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.0",
        "webpack-dev-server": "^4.11.1"
    },
    "consolePlugin": {
        "name": "camel-openshift-console-plugin",
        "version": "0.0.1",
        "displayName": "Camel OpenShift Console Plugin",
        "description": "Camel plugin for the OpenShift Console",
        "exposedModules": {
            "HomePage": "./pages/HomePage",
            "ApplicationPage": "./pages/ApplicationPage",
            "actions": "./actions"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    },
    "dependencies": {
        "mocha": "^10.5.1",
        "js-base64": "^3.7.6",
        "rxjs": "^7.8.1",
        "zustand": "^4.4.7"
    },
    "resolutions": {
      "@types/react": "^17.0.37",
      "webpack": "^5.75.0"
    }
}